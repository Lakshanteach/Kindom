<!DOCTYPE html>
<html><head><title>Dashboard | Kingdom Panel</title></head><body>
  <h1>Kingdom of Devil Panel</h1>
  <form method="post" action="/logout"><button type="submit">Logout</button></form>
  <% if (error) { %><p style="color:red"><%= error %></p><% } %>
  <table border="1"><tr><th>Name</th><th>ID</th><th>Status</th></tr>
  <% servers.forEach(s => { %>
    <tr>
      <td><%= s.attributes.name %></td>
      <td><%= s.attributes.identifier %></td>
      <td><%= s.attributes.status %></td>
    </tr>
  <% }) %></table>
</body></html>